        <td>
            <%=i+1 %>
        </td>
        <td>
            <%=users[i].name %>
        </td>
        <td>
            <%=users[i].email %>
        </td>
        <td>
            <%=users[i].mobile %>
        </td>
        <td>
            <%=users[i].gender %>
        </td>
        <td>
            <%=users[i].status %>
        </td>
        <td>
            <a href="/update-user?id=<%=users[i]._id%>" class="btn border-shodow update ">
                <span class="text-geadient ">
            <i class="fas fa-pencil-alt "></i>
        </span>
            </a>
            <a data-id="<%=users[i]._id%>" class="btn border-shodow delete ">
                <span class="text-geadient ">
            <i class="fas fa-trash-alt "></i>
        </span>
